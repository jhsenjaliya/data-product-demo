{
  "$id": "https://example.com/user/profile.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserDomain",
  "type": "object",
  "required": [
    "user_id",
    "full_name",
    "email"
  ],
  "properties": {
    "user_id": {
      "type": "string"
    },
    "full_name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "dob": {
      "type": "string",
      "format": "date"
    },
    "phone": {
      "type": "string"
    }
  }
}
