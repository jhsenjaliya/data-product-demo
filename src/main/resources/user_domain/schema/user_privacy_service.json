{
  "$id": "https://example.com/user/privacy.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserPrivacy",
  "type": "object",
  "properties": {
    "gdpr_consent": {
      "type": "boolean"
    },
    "marketing_consent": {
      "type": "boolean"
    },
    "consent_timestamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "gdpr_consent",
    "consent_timestamp"
  ]
}
